<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.39.3">
  <project>
    <settings>
      <setting id="name">PasswordReset</setting>
      <setting id="description">This will reset your password</setting>
      <setting id="module_name">PasswordReset</setting>
      <setting id="template">application</setting>
      <setting id="main_widget">password_reset</setting>
      <setting id="main_classname">PasswordReset</setting>
      <setting id="main_menu" />
      <setting id="output_dir" />
      <setting id="output_dir2" />
      <setting id="import_tkvariables">False</setting>
      <setting id="use_ttk_styledefinition_file">False</setting>
      <setting id="use_i18n">False</setting>
      <setting id="all_ids_attributes">False</setting>
      <setting id="generate_code_onsave">False</setting>
      <setting id="use_window_centering_code">False</setting>
      <setting id="ttk_style_definition_file" />
    </settings>
    <customwidgets />
  </project>
  <object class="tk.Toplevel" id="password_reset" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <child>
      <object class="ttk.Frame" id="password_frame" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="reset" named="True">
            <property name="font">system</property>
            <property name="padding">10</property>
            <property name="text" translatable="yes">Reset Password</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator_password" named="True">
            <property name="orient">horizontal</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="password_label" named="True">
            <property name="anchor">center</property>
            <property name="text" translatable="yes">Password:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="password_enter" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">30</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="confirm_label" named="True">
            <property name="anchor">center</property>
            <property name="text" translatable="yes">Confirm Password:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">20</property>
              <property name="pady">10</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="confirm_enter" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="password_button" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Button" id="confirm_button" named="True">
            <property name="command" type="command" cbtype="simple">forget_password_helper</property>
            <property name="text" translatable="yes">Confirm!</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">50</property>
              <property name="pady">15</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="cancel_button" named="True">
            <property name="command" type="command" cbtype="simple">cancel</property>
            <property name="text" translatable="yes">Cancel</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">50</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
