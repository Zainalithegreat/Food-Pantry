<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.39.3">
  <project>
    <settings />
    <customwidgets />
  </project>
  <object class="tk.Toplevel" id="main" named="True">
    <property name="background">#c0c0c0</property>
    <property name="height">400</property>
    <property name="minsize">500|300</property>
    <property name="padx">50</property>
    <property name="pady">50</property>
    <property name="title" translatable="yes">PCC Food Pantry Notification System</property>
    <property name="width">500</property>
    <child>
      <object class="ttk.Frame" id="entry" named="True">
        <property name="height">200</property>
        <property name="width">300</property>
        <layout manager="grid" />
        <child>
          <object class="ttk.Label" id="title_label" named="True">
            <property name="font">system</property>
            <property name="padding">10</property>
            <property name="text" translatable="yes">Welcome to PCC's Food Pantry Notification System</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">4</property>
              <property name="row">0</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator1">
            <property name="orient">horizontal</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">4</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="user_label" named="True">
            <property name="anchor">center</property>
            <property name="text" translatable="yes">Username or Email:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">20</property>
              <property name="pady">8</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="user_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">5</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="pass_label" named="True">
            <property name="anchor">center</property>
            <property name="text" translatable="yes">Password:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">5</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="pass_entry" named="True">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">10</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="login_button" named="True">
            <property name="command" type="command" cbtype="simple">user_login</property>
            <property name="text" translatable="yes">Login</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">15</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="register_button" named="True">
            <property name="command" type="command" cbtype="simple">user_register</property>
            <property name="text" translatable="yes">Register</property>
            <property name="width">10</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">1</property>
              <property name="padx">10</property>
              <property name="row">4</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="reset_password" named="True">
            <property name="command" type="command" cbtype="simple">forget_password</property>
            <property name="text" translatable="yes">Reset Password</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">10</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
