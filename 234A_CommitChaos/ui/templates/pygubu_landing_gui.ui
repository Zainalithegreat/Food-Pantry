<?xml version='1.0' encoding='utf-8'?>
<interface version="1.4" author="PygubuDesigner 0.39.3">
  <project>
    <settings />
    <customwidgets />
  </project>
  <object class="tk.Toplevel" id="toplevel1">
    <property name="background">#c0c0c0</property>
    <property name="height">150</property>
    <property name="minsize">350|350</property>
    <property name="title" translatable="yes">PCC Food Pantry Notification System</property>
    <property name="width">150</property>
    <containerlayout manager="grid">
      <property name="anchor">center</property>
      <property type="col" id="0" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">1</property>
          <property name="sticky">ew</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">center</property>
        </containerlayout>
        <child>
          <object class="ttk.Button" id="notif_btn" named="True">
            <property name="command" type="command" cbtype="simple">notification_creation</property>
            <property name="text" translatable="yes">Send Notification</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="template_btn" named="True">
            <property name="command" type="command" cbtype="simple">template_creation</property>
            <property name="text" translatable="yes">Template Creation</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">15</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="notif_rvw_btn" named="True">
            <property name="command" type="command" cbtype="simple">notification_review</property>
            <property name="text" translatable="yes">Notification Log</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="row">2</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="subscriber_button" named="True">
            <property name="command" type="command" cbtype="simple">subscriber_page</property>
            <property name="text" translatable="yes">   Subscriber Page  </property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="pady">10</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">5</property>
          <property name="row">2</property>
          <property name="sticky">ew</property>
        </layout>
        <containerlayout manager="grid">
          <property name="anchor">center</property>
        </containerlayout>
        <child>
          <object class="ttk.Button" id="button1">
            <property name="command" type="command" cbtype="simple">on_exit_button_clicked</property>
            <property name="text" translatable="yes">Exit</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame4">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">5</property>
          <property name="row">0</property>
          <property name="sticky">ew</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label2">
            <property name="font">{Arial Rounded MT Bold} 12 {}</property>
            <property name="padding">10</property>
            <property name="text" translatable="yes">Main Menu</property>
            <layout manager="pack" />
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
