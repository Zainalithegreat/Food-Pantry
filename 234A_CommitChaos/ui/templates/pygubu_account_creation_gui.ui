<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel">
    <property name="title" translatable="yes">User Registration</property>
    <property name="background">#c0c0c0</property>
    <property name="height">400</property>
    <property name="minsize">400|400</property>
    <property name="width">400</property>
    <child>
      <object class="ttk.Frame" id="frame3">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">50</property>
          <property name="pady">50</property>
          <property name="row">0</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="1" name="pad">15</property>
          <property type="col" id="1" name="uniform">0</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="title_label">
            <property name="font">system</property>
            <property name="text" translatable="yes">Please fill out the information below</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="ipady">5</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator1">
            <property name="orient">horizontal</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="row">1</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="name_label">
            <property name="text" translatable="yes">Full Name:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">11</property>
              <property name="pady">5</property>
              <property name="row">2</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="user_label" named="True">
            <property name="text" translatable="yes">Username:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">11</property>
              <property name="pady">5</property>
              <property name="row">3</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="email_label" named="True">
            <property name="text" translatable="yes">Email:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">11</property>
              <property name="pady">5</property>
              <property name="row">4</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="pass_label" named="True">
            <property name="text" translatable="yes">Password:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">11</property>
              <property name="pady">5</property>
              <property name="row">5</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="pass_confirm_label" named="True">
            <property name="text" translatable="yes">Confirm Password:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">11</property>
              <property name="pady">5</property>
              <property name="row">6</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="name_entry">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">5</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="user_entry">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">5</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="email_entry">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">5</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="pass_entry">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">5</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="pass_confirm_entry">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">5</property>
              <property name="row">6</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Separator" id="separator2">
            <property name="orient">horizontal</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="columnspan">2</property>
              <property name="row">7</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="back_button">
            <property name="command" type="command" cbtype="simple">back_to_login</property>
            <property name="text" translatable="yes">Back to login</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="ipadx">15</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="row">8</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="register_button">
            <property name="command" type="command" cbtype="simple">confirm_registration</property>
            <property name="text" translatable="yes">Register</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="ipadx">15</property>
              <property name="padx">15</property>
              <property name="pady">15</property>
              <property name="row">8</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
